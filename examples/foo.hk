module Foo;

import Bar (bar, baz);

foreign import "inspect" (inspect : forall a. a -> String);

foo = bar baz;

myInspect = inspect;

resultOfInspect = inspect foo;

luaAdd : Int -> Int -> IO Int;
luaAdd a b = lua { a + b };

luaCallMethodDelete : forall a. a -> IO ();
luaCallMethodDelete obj = lua { obj:delete() };
