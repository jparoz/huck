(* isTrue True = True; *)
(* isTrue _ = False; *)

isFive 5 = True;
isFive _ = False;

isHello ("Hello!") = True;
isHello _ = False;

(* Cons : A -> B -> Cons; *)
(* head : Cons -> A; *)
head (Cons x xs) = x;
head Nil = undefined;

head (x::xs) = x;
head [] = undefined;

length (x::xs) = 1 + length xs;
length [] = 0;

isSingleton [x] = True;
isSingleton _ = False;

